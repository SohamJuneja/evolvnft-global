# EvolvNFT Environment Variables Example
# Copy this to .env and fill in your actual values

# Oracle Wallet Private Key (without 0x prefix)
PRIVATE_KEY=your_oracle_wallet_private_key_here

# Dedicated RPC URL (replace with your Alchemy/Infura URL)
# Somnia Testnet RPC Configuration
# Use Ankr dedicated RPC endpoint (RECOMMENDED)
SOMNIA_RPC_URL=https://rpc.ankr.com/somnia_testnet/6d98f304a89a1bae8c99518de1733780853d152e34f4f110555e94c4514d47c8

# Alternative RPC endpoints (fallback options):
# SOMNIA_RPC_URL=https://somnia-testnet.g.alchemy.com/v2/YOUR_SECRET_KEY
# SOMNIA_RPC_URL=https://somnia-testnet.infura.io/v3/YOUR_PROJECT_ID
# SOMNIA_RPC_URL=https://somnia-testnet.blockscout.com/api/eth-rpc
# Alternative providers:
# SOMNIA_RPC_URL=https://somnia-testnet.infura.io/v3/YOUR_PROJECT_ID
# SOMNIA_RPC_URL=https://rpc.somnia.network

# Oracle Configuration
NODE_ENV=production
EVOLUTION_INTERVAL_MINUTES=10
MAX_TOKENS_PER_RUN=5
SINGLE_RUN=false

# Optional: Weather API (for real weather data)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Contract Address (already deployed)
CONTRACT_ADDRESS=0xED32eAE05bdcB1fDabB02b0E0fb4148eFDa486c9